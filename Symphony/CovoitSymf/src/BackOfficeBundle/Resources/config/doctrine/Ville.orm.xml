<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BackOfficeBundle\Entity\Ville" table="ville">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codeInsee" type="string" column="code_insee" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ville" type="string" column="ville" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codePostal" type="string" column="code_postal" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dep" type="string" column="dep" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
